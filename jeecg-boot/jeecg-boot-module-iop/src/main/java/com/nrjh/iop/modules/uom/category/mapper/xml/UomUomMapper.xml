<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nrjh.iop.modules.uom.category.mapper.UomUomMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  uom_uom 
		WHERE
			 category_id = #{mainId} 	</delete>
	
	<select id="selectByMainId" parameterType="java.lang.String" resultType="com.nrjh.iop.modules.uom.category.entity.UomUom">
		SELECT * 
		FROM  uom_uom
		WHERE
			 category_id = #{mainId} 	</select>
</mapper>
